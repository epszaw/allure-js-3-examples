{
  "ts-node": {
    "files": true
  },
  "compilerOptions": {
    "target": "es2018",
    "lib": ["es2018", "DOM"],
    "esModuleInterop": true,
    "module": "Node16",
    "moduleResolution": "Node16",
    "strictNullChecks": false,
    "types": ["cypress", "node"],
    "declaration": true,
    "skipLibCheck": true,
    "preserveValueImports": false
  },
  "exclude": ["node_modules"]
}
